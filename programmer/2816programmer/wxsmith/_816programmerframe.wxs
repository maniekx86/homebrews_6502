						<object class="wxStaticText" name="ID_STATICTEXT5" variable="StaticText4" member="yes">
							<label>File to read EEPROM</label>
							<pos>16,264</pos>
						</object>
						<object class="wxButton" name="ID_BUTTON2" variable="startButton" member="yes">
							<label>Start</label>
							<pos>72,328</pos>
							<size>208,40</size>
							<handler function="OnstartButtonClick" entry="EVT_BUTTON" />
						</object>
						<object class="wxRadioBox" name="ID_RADIOBOX2" variable="ROMTypeSelector" member="yes">
							<label>EEPROM Type</label>
							<content>
								<item>28C16 (2 KB)</item>
								<item>28C256 (32 KB) (Only 16 KB reads/writes)</item>
							</content>
							<default>-1</default>
							<pos>10,16</pos>
							<size>320,79</size>
							<style>wxRA_HORIZONTAL</style>
						</object>
					</object>
					<object class="wxComboBox" name="ID_COMBOBOX1" variable="SerialList" member="yes">
						<pos>16,16</pos>
						<size>199,31</size>
						<style>wxCB_READONLY</style>
					</object>
					<object class="wxStaticText" name="ID_STATICTEXT2" variable="SerialState" member="yes">
						<label>State: disconnected</label>
						<pos>16,56</pos>
						<size>311,17</size>
					</object>
				</object>
				<col>0</col>
				<row>2</row>
				<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxStaticText" name="ID_STATICTEXT1" variable="StaticText1" member="yes">
