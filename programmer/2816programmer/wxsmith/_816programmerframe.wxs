<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="_816programmerFrame">
		<title>2816/256 Programmer</title>
		<size>359,542</size>
		<id_arg>0</id_arg>
		<style>wxCLOSE_BOX|wxMINIMIZE_BOX</style>
		<object class="wxGridBagSizer" variable="GridBagSizer1" member="no">
			<object class="sizeritem">
				<object class="wxPanel" name="ID_PANEL1" variable="Panel1" member="yes">
					<size>349,494</size>
					<object class="wxButton" name="ID_BUTTON1" variable="ConnectButton" member="yes">
						<label>Connect</label>
						<pos>224,17</pos>
						<size>112,34</size>
						<handler function="OnConnectButtonClick" entry="EVT_BUTTON" />
					</object>
					<object class="wxPanel" name="ID_PANEL2" variable="Panel2" member="yes">
						<pos>0,80</pos>
						<size>347,408</size>
						<enabled>0</enabled>
						<style>wxBORDER_SIMPLE|wxTAB_TRAVERSAL</style>
						<object class="wxRadioBox" name="ID_RADIOBOX1" variable="ActionSelector" member="yes">
							<label>Action</label>
							<content>
								<item>Read EEPROM</item>
								<item>Verify EEPROM</item>
								<item>Write EEPROM</item>
							</content>
							<default>-1</default>
							<pos>8,104</pos>
							<size>328,96</size>
							<style>wxRA_HORIZONTAL</style>
						</object>
						<object class="wxGauge" name="ID_GAUGE1" variable="progressGauge" member="yes">
							<range>2048</range>
							<pos>16,376</pos>
							<size>312,16</size>
						</object>
						<object class="wxStaticText" name="ID_STATICTEXT3" variable="progressText" member="yes">
							<label>Progress: -</label>
							<pos>16,368</pos>
						</object>
						<object class="wxFilePickerCtrl" name="ID_FILEPICKERCTRL1" variable="readFile" member="yes">
							<message></message>
							<wildcard></wildcard>
							<pos>16,280</pos>
							<size>312,34</size>
							<style>wxFLP_OVERWRITE_PROMPT|wxFLP_SAVE|wxFLP_USE_TEXTCTRL</style>
						</object>
						<object class="wxFilePickerCtrl" name="ID_FILEPICKERCTRL2" variable="writeverifyFile" member="yes">
							<message></message>
							<wildcard></wildcard>
							<pos>16,224</pos>
							<size>312,34</size>
							<style>wxFLP_FILE_MUST_EXIST|wxFLP_OPEN|wxFLP_USE_TEXTCTRL</style>
						</object>
						<object class="wxStaticText" name="ID_STATICTEXT4" variable="StaticText3" member="yes">
